{
  "name": "@codecabana/web-components",
  "version": "0.0.52",
  "author": "Code Cabana",
  "license": "MIT",
  "description": "Web components designed to infiltrate no-code platforms via script tags",
  "homepage": "https://codecabana.com.au",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": "./dist/esm/index.js",
    "./src": "./src/components/index.tsx"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "run-s build_*",
    "build_esm": "tsc --project ./ci/tsconfig.esm.json",
    "build_browser": "tsc --project ./ci/tsconfig.browser.json",
    "test": "run-s size",
    "size": "bundlewatch --config ci/bundlewatch.config.json"
  },
  "devDependencies": {
    "@atomico/hooks": "^3.36.0",
    "@types/node": "^17.0.34",
    "@uppercod/esbuild-jsx-runtime": "^1.1.1",
    "atomico": "^1.59.0",
    "bundlewatch": "^0.3.3",
    "esbuild": "^0.14.39",
    "fast-glob": "^3.2.11",
    "sass": "^1.51.0",
    "typedoc": "^0.22.15"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-cabana/web-components.git"
  },
  "keywords": [
    "web-components",
    "atomico"
  ]
}
