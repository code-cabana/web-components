{
  "name": "@codecabana/web-components",
  "version": "0.0.52",
  "author": "Code Cabana",
  "license": "MIT",
  "description": "Web components designed to infiltrate no-code platforms via script tags",
  "homepage": "https://codecabana.com.au",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": "./dist/esm/index.jsx",
    "./types": "./dist/esm/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev_tsc dev_esbuild",
    "dev_tsc": "npm run build_tsc -- --watch",
    "dev_esbuild": "node ./ci/esbuild.dev.js",
    "build": "run-s build_*",
    "build_tsc": "tsc",
    "build_esbuild": "node ./ci/esbuild.prod.js",
    "test": "run-s test_*",
    "test_size": "bundlewatch --config ci/bundlewatch.config.json"
  },
  "devDependencies": {
    "@atomico/hooks": "^3.36.0",
    "@types/node": "^17.0.34",
    "@uppercod/esbuild-jsx-runtime": "^1.1.1",
    "atomico": "^1.59.0",
    "bundlewatch": "^0.3.3",
    "esbuild": "^0.14.39",
    "sass": "^1.51.0",
    "typedoc": "^0.22.15"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-cabana/web-components.git"
  },
  "keywords": [
    "web-components",
    "atomico"
  ]
}
